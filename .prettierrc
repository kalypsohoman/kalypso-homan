{
	"semi": true,
	"trailingComma": "es5",
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
	"bracketSpacing": true,
	"useTabs": true,
	"svelteSortOrder": "options-scripts-markup-styles",
	"svelteStrictMode": false,
	"svelteBracketNewLine": true,
	"svelteAllowShorthand": false,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
	  {
		"files": "*.svelte",
		"options": {
		  "parser": "svelte"
		}
	  }
	]
  }